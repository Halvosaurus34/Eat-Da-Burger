<div class="container">
<h1 class="display-4">Eat-Da-Burger!</h1>
<h3>Devoured</h3>
<ul>
  {{#each devoured}}
  <li>
    <p>
      {{this.burger_name}}
    </p>
  </li>
  {{/each}}
</ul>
<h3>Waiting to be devoured</h3>
<ul>
  {{#each notDevoured}}
  <li>
    <p>
      {{this.burger_name}}
        <button id="eatButton{{this.id}}">Eat Burger</button>

    </p>
  </li>
  {{/each}}
</ul>
<h2>Add a Burger</h2>
<form class="create-form">

  <div class="form-group">
    <label for="ca">Burger Name:</label>
    <input type="text" id="burgerName" name="burgerName">
  </div>

  <button type="submit" onClick="addBurger($('#burgerName').val())">Add Burger</button>
</form>


</div>